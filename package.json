{
  "name": "oilmod-web-nx",
  "version": "20.0.13",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx run-many --target=build  --projects=core,common,datatable,drawer,error-handler,form,popover",
    "build:watch": "nx watch --all -- nx run %NX_PROJECT_NAME%:build",
    "build:styles": "ts-node --project tools/tsconfig.tools.json tools/scripts/build-scss.ts",
    "test": "nx run-many --target=test --all --silent",
    "lint": "nx run-many --target=lint --all",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest && nx migrate --run-migrations --if-exists && git commit -am 'chore: update dependencies' && bunx npm-check-updates --format group -u --cooldown 5 --target minor && bun install",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "publish:all": "bun run build && (cd dist/libs/core && bun publish || echo skipped core) && (cd dist/libs/common && bun publish || echo skipped common) && (cd dist/libs/datatable && bun publish || echo skipped datatable) && (cd dist/libs/drawer && bun publish || echo skipped drawer) && (cd dist/libs/error-handler && bun publish || echo skipped error-handler) && (cd dist/libs/form && bun publish || echo skipped form) && (cd dist/libs/popover && bun publish || echo skipped popover) && (cd dist/libs/testing && bun publish || echo skipped testing)",
    "storybook": "nx run oilmod-web:storybook",
    "storybook:build": "nx run oilmod-web:build-storybook",
    "clean": "rm -rf .angular .cache && bun run nx reset",
    "yalc:publish": "bash -c 'for dir in dist/libs/*; do cd \"$dir\" && yalc publish && cd -; done'",
    "yalc:push": "bash -c '[ -f dist/libs/core/fesm2022/ngx-stoui-core.mjs ] && for dir in dist/libs/*; do cd \"$dir\" && yalc push --sig && cd -; done || echo \"Build in progress, skipping push...\"'",
    "yalc:watch": "chokidar \"dist/libs/*/fesm2022/*.mjs\" --debounce 8000 --polling -c \"bun run yalc:push\"",
    "dev": "NX_TUI=false bun run build && bun run yalc:push && concurrently -n build,yalc -c blue,green \"bun run build:watch\" \"bun run yalc:watch\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "21.0.6",
    "@angular/build": "21.0.3",
    "@angular/cdk": "21.0.3",
    "@angular/common": "21.0.6",
    "@angular/compiler": "21.0.6",
