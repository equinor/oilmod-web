    "@angular/platform-browser-dynamic": "21.0.6",
    "@angular/router": "21.0.6",
    "@ngx-stoui/swimlane-datatable": "20.0.0",
    "@nx/angular": "22.3.0",
    "@storybook/builder-vite": "10.1.10",
    "date-fns": "4.1.0",
    "rxjs": "7.8.2",
