{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@ngx-stoui/datatable": [
        "dist/stoui-datatable"
      ],
      "@ngx-stoui/datatable/*": [
        "dist/stoui-datatable/*"
      ],
      "@ngx-stoui/core": [
        "dist/stoui-core"
      ],
      "@ngx-stoui/core/*": [
        "dist/stoui-core"
      ],
      "@ngx-stoui/form": [
        "dist/stoui-form"
      ],
      "@ngx-stoui/form/*": [
        "dist/stoui-form/*"
      ],
      "@ngx-stoui/common": [
        "dist/stoui-common"
      ],
      "@ngx-stoui/common/*": [
        "dist/stoui-common/*"
      ],
      "@ngx-stoui/drawer": [
        "dist/stoui-drawer"
      ],
      "@ngx-stoui/drawer/*": [
        "dist/stoui-drawer/*"
      ],
      "@ngx-stoui/error-handler": [
        "dist/stoui-error-handler"
      ],
      "@ngx-stoui/error-handler/*": [
        "dist/stoui-error-handler/*"
      ],
      "@ngx-stoui/quick-view": [
        "dist/stoui-quick-view"
      ],
      "@ngx-stoui/quick-view/*": [
        "dist/stoui-quick-view/*"
      ],
      "@ngx-stoui/unsaved-changes": [
        "dist/stoui-unsaved-changes"
      ],
      "@ngx-stoui/unsaved-changes/*": [
        "dist/stoui-unsaved-changes/*"
      ],
      "@testing": [
        "testing"
      ],
      "@testing/*": [
        "testing/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": false
  }
}
