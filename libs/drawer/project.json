{
  "name": "drawer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/drawer/src",
  "prefix": "sto",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "options": {
        "project": "libs/drawer/ng-package.json",
        "updateBuildableProjectDepsInPackageJson": false
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/drawer/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/drawer/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/drawer"],
      "options": {
        "jestConfig": "libs/drawer/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/drawer/src/**/*.ts",
          "libs/drawer/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "dist/libs/drawer",
        "commands": ["npm publish"],
        "parallel": false
      }
    }
  },
  "tags": []
}
